class Solution:
    def preorderTraversal(self, root: Optional[TreeNode]) -> List[int]:
        res = []
        if not root: 
            return res     
        def dfs(root):
            res.append(root.val) 
            if root.left: 
                dfs(root.left)   
            if root.right: 
                dfs(root.right)
        dfs(root)
        return res     
