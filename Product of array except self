class Solution:
    def productExceptSelf(self, nums: List[int]) -> List[int]:
        n = len(nums)
        no_of_zeros = nums.count(0)
        prod = 1
        for num in nums:
            if num != 0:
                prod *= num
        ans = []
        for num in nums:
            if no_of_zeros > 1:
                ans.append(0)
            elif no_of_zeros == 1:
                ans.append(prod if num == 0 else 0)
            else:
                ans.append(prod // num)
        return ans
